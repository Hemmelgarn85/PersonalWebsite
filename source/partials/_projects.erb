      <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="projects">
        <div class="my-auto">
          <h2 class="mb-5">Projects</h2>
          <ul class="fa-ul mb-0">
            <% data.projects.projects.each do |a| %>
            <h3 class=""><%= a.project %> -  <a class="mb-4" href=<%= a.link %>>Github</a></h3>
            <% a.imgs.each do |b| %>
            <img class="img-fluid img-profile mx-auto mb-2" src=<%= b.img %> alt=<%= b.alt %>>
            <% end %>
            <% end %>

            
            
          </ul>
        </div>
      </section>

    </div>
